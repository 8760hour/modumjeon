<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--  SQL Mapping 설정 -->
<mapper namespace="kakaoMap">
    <!--  SQL 객체 -->
    <!--    게시글 목록 -->
    <select id="selectjson" resultType="KakaoMapDto">
    SELECT TOP(5) 'totalcount'
	FROM Shop
	WHERE ADDRESS LIKE '경기도 김포시%'
	</select>
</mapper>